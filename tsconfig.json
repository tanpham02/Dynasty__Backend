{
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "outDir": "build",
    "rootDir": "src",
    "lib": ["es6"],
    "allowJs": true,
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@app/*":["./src/*"],
    }

  },
  "include": ["src/**/*.ts", "*/upload/**/*"],
  "exclude": ["node_modules"],
 }


