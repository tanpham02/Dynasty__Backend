{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "paths": {
      "@app/*":["./src/*"]
    }
   
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"],
}

